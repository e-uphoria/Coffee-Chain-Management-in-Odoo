<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:bioc="http://bpmn.io/schema/bpmn/biocolor/1.0" xmlns:color="http://www.omg.org/spec/BPMN/non-normative/color/1.0" id="Definitions_SupplyChainSnake" targetNamespace="http://example.com/odoo/supplychain" exporter="Camunda Modeler" exporterVersion="5.37.0">
  <bpmn:collaboration id="Collab_SupplyChain">
    <bpmn:participant id="Participant_SupplyChain" name="Odoo Supply Chain" processRef="SupplyChainSnakeFlow" />
  </bpmn:collaboration>
  <bpmn:process id="SupplyChainSnakeFlow" name="Odoo Supply Chain - Snake Portrait" isExecutable="false">
    <bpmn:laneSet id="LaneSet_SupplyChain">
      <bpmn:lane id="Lane_Procurement" name="Procurement">
        <bpmn:flowNodeRef>StartEvent</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_CreatePO</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_GoodsReceipt</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Inventory" name="Inventory">
        <bpmn:flowNodeRef>Task_UpdateStock</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_UpdateInventory</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Manufacturing" name="Manufacturing">
        <bpmn:flowNodeRef>Task_CreateMO</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Manufacture</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_FinishProd</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_PLM" name="Engineering / PLM">
        <bpmn:flowNodeRef>Task_QualityInspection</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_PLM</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Quality" name="Quality Control">
        <bpmn:flowNodeRef>Gateway_QCRequired</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_QCResult</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ApproveBatch</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Maintenance" name="Maintenance / Rework">
        <bpmn:flowNodeRef>Task_QualityReinspect</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_CreateMaintenance</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_MaintenanceDo</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>
    <bpmn:startEvent id="StartEvent" name="Purchase Request Received">
      <bpmn:outgoing>Flow_Start_PO</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:task id="Task_CreatePO" name="Create Purchase Order">
      <bpmn:incoming>Flow_Start_PO</bpmn:incoming>
      <bpmn:outgoing>Flow_PO_GR</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_GoodsReceipt" name="Goods Receipt / Putaway">
      <bpmn:incoming>Flow_PO_GR</bpmn:incoming>
      <bpmn:outgoing>Flow_GR_UpdateStock</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_UpdateStock" name="Update Stock &#38; Location">
      <bpmn:incoming>Flow_GR_UpdateStock</bpmn:incoming>
      <bpmn:outgoing>Flow_UpdateStock_CreateMO</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_CreateMO" name="Create Manufacturing Order">
      <bpmn:incoming>Flow_UpdateStock_CreateMO</bpmn:incoming>
      <bpmn:outgoing>Flow_CreateMO_Manufacture</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_Manufacture" name="Execute Production">
      <bpmn:incoming>Flow_CreateMO_Manufacture</bpmn:incoming>
      <bpmn:outgoing>Flow_Manufacture_Finish</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_FinishProd" name="Finish Production / Update Qty">
      <bpmn:incoming>Flow_Manufacture_Finish</bpmn:incoming>
      <bpmn:outgoing>Flow_Finish_PLM</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_PLM" name="Engineering Change / BOM Update">
      <bpmn:incoming>Flow_Finish_PLM</bpmn:incoming>
      <bpmn:outgoing>Flow_PLM_QCRequired</bpmn:outgoing>
    </bpmn:task>
    <bpmn:exclusiveGateway id="Gateway_QCRequired" name="QC Required?">
      <bpmn:incoming>Flow_PLM_QCRequired</bpmn:incoming>
      <bpmn:outgoing>Flow_QCReq_Yes</bpmn:outgoing>
      <bpmn:outgoing>Flow_QCReq_No</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:task id="Task_QualityInspection" name="Quality Inspection">
      <bpmn:incoming>Flow_QCReq_Yes</bpmn:incoming>
      <bpmn:outgoing>Flow_QI_QCResult</bpmn:outgoing>
    </bpmn:task>
    <bpmn:exclusiveGateway id="Gateway_QCResult" name="QC Passed?">
      <bpmn:incoming>Flow_QI_QCResult</bpmn:incoming>
      <bpmn:incoming>Flow_1qmkk1u</bpmn:incoming>
      <bpmn:outgoing>Flow_QC_Pass</bpmn:outgoing>
      <bpmn:outgoing>Flow_QC_Fail</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:task id="Task_ApproveBatch" name="Approve / Release Batch">
      <bpmn:incoming>Flow_QC_Pass</bpmn:incoming>
      <bpmn:outgoing>Flow_Approve_UpdateInv</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_UpdateInventory" name="Update Inventory (Available to Sell)">
      <bpmn:incoming>Flow_Approve_UpdateInv</bpmn:incoming>
      <bpmn:incoming>Flow_QCReq_No</bpmn:incoming>
      <bpmn:outgoing>Flow_UpdateInv_End</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_CreateMaintenance" name="Create Maintenance / Rework">
      <bpmn:incoming>Flow_QC_Fail</bpmn:incoming>
      <bpmn:outgoing>Flow_CreateMaint_Do</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_MaintenanceDo" name="Perform Maintenance / Rework">
      <bpmn:incoming>Flow_CreateMaint_Do</bpmn:incoming>
      <bpmn:outgoing>Flow_Do_Reinspect</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_QualityReinspect" name="Reinspect after Rework">
      <bpmn:incoming>Flow_Do_Reinspect</bpmn:incoming>
      <bpmn:outgoing>Flow_1qmkk1u</bpmn:outgoing>
    </bpmn:task>
    <bpmn:endEvent id="EndEvent" name="Supply Chain Flow Complete">
      <bpmn:incoming>Flow_UpdateInv_End</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_Start_PO" sourceRef="StartEvent" targetRef="Task_CreatePO" />
    <bpmn:sequenceFlow id="Flow_PO_GR" sourceRef="Task_CreatePO" targetRef="Task_GoodsReceipt" />
    <bpmn:sequenceFlow id="Flow_GR_UpdateStock" sourceRef="Task_GoodsReceipt" targetRef="Task_UpdateStock" />
    <bpmn:sequenceFlow id="Flow_UpdateStock_CreateMO" sourceRef="Task_UpdateStock" targetRef="Task_CreateMO" />
    <bpmn:sequenceFlow id="Flow_CreateMO_Manufacture" sourceRef="Task_CreateMO" targetRef="Task_Manufacture" />
    <bpmn:sequenceFlow id="Flow_Manufacture_Finish" sourceRef="Task_Manufacture" targetRef="Task_FinishProd" />
    <bpmn:sequenceFlow id="Flow_Finish_PLM" sourceRef="Task_FinishProd" targetRef="Task_PLM" />
    <bpmn:sequenceFlow id="Flow_PLM_QCRequired" sourceRef="Task_PLM" targetRef="Gateway_QCRequired" />
    <bpmn:sequenceFlow id="Flow_QCReq_Yes" name="Yes" sourceRef="Gateway_QCRequired" targetRef="Task_QualityInspection" />
    <bpmn:sequenceFlow id="Flow_QCReq_No" name="No" sourceRef="Gateway_QCRequired" targetRef="Task_UpdateInventory" />
    <bpmn:sequenceFlow id="Flow_QI_QCResult" sourceRef="Task_QualityInspection" targetRef="Gateway_QCResult" />
    <bpmn:sequenceFlow id="Flow_QC_Pass" name="Pass" sourceRef="Gateway_QCResult" targetRef="Task_ApproveBatch" />
    <bpmn:sequenceFlow id="Flow_QC_Fail" name="Fail" sourceRef="Gateway_QCResult" targetRef="Task_CreateMaintenance" />
    <bpmn:sequenceFlow id="Flow_Approve_UpdateInv" sourceRef="Task_ApproveBatch" targetRef="Task_UpdateInventory" />
    <bpmn:sequenceFlow id="Flow_UpdateInv_End" sourceRef="Task_UpdateInventory" targetRef="EndEvent" />
    <bpmn:sequenceFlow id="Flow_CreateMaint_Do" sourceRef="Task_CreateMaintenance" targetRef="Task_MaintenanceDo" />
    <bpmn:sequenceFlow id="Flow_Do_Reinspect" sourceRef="Task_MaintenanceDo" targetRef="Task_QualityReinspect" />
    <bpmn:sequenceFlow id="Flow_1qmkk1u" sourceRef="Task_QualityReinspect" targetRef="Gateway_QCResult" />
  </bpmn:process>
