<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" id="Definitions_CompanySales_v2" targetNamespace="http://example.com/bpmn" exporter="Camunda Modeler" exporterVersion="5.37.0">
  <bpmn:collaboration id="Collab_Sales">
    <bpmn:participant id="Participant_Customer" name="Customer" processRef="Process_Customer" />
    <bpmn:participant id="Participant_Company" name="Company" processRef="Process_Company" />
    <bpmn:messageFlow id="MF_Order" sourceRef="Task_PlaceOrder" targetRef="Task_ReceiveOrder" />
    <bpmn:messageFlow id="MF_Payment" sourceRef="Task_MakePayment" targetRef="Task_RecordPayment" />
    <bpmn:messageFlow id="MF_OrderReady" sourceRef="Task_HandOverOrder" targetRef="Task_ReceiveOrderCustomer" />
    <bpmn:messageFlow id="MF_QuoteReq" sourceRef="Task_GenerateQuotation" targetRef="Task_ApproveQuotation" />
  </bpmn:collaboration>
  <bpmn:process id="Process_Customer" name="Customer Journey" isExecutable="false">
    <bpmn:startEvent id="Start_Customer" name="Start Order" />
    <bpmn:sendTask id="Task_PlaceOrder" name="Place Order (Online/Outlet)" />
    <bpmn:userTask id="Task_ApproveQuotation" name="Approve Quotation?" />
    <bpmn:userTask id="Task_MakePayment" name="Make Payment" />
    <bpmn:receiveTask id="Task_ReceiveOrderCustomer" name="Receive Order / Delivery" />
    <bpmn:endEvent id="End_Customer" name="Order Completed" />
    <bpmn:sequenceFlow id="SF_C1" sourceRef="Start_Customer" targetRef="Task_PlaceOrder" />
    <bpmn:sequenceFlow id="SF_C2" sourceRef="Task_PlaceOrder" targetRef="Task_ApproveQuotation" />
    <bpmn:sequenceFlow id="SF_C2a" sourceRef="Task_ApproveQuotation" targetRef="Task_MakePayment" />
    <bpmn:sequenceFlow id="SF_C3" sourceRef="Task_MakePayment" targetRef="Task_ReceiveOrderCustomer" />
    <bpmn:sequenceFlow id="SF_C4" sourceRef="Task_ReceiveOrderCustomer" targetRef="End_Customer" />
  </bpmn:process>
  <bpmn:process id="Process_Company" name="Company Sales Process" isExecutable="true">
    <bpmn:laneSet id="LaneSet_Company">
      <bpmn:lane id="Lane_Warehouse" name="Warehouse / Inventory">
        <bpmn:flowNodeRef>Task_UpdateInventory</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>End_Company</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_TriggerRestock</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_StockLow</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Accounting" name="Accounting / Finance">
        <bpmn:flowNodeRef>Task_RecordPayment</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_GenerateInvoice</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>End_PaymentFailed</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_HandOverOrder</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_PaymentSuccess</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_CRM" name="Sales &#38; CRM">
        <bpmn:flowNodeRef>Task_IdentifyCustomer</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_CheckLoyalty</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ApplyDiscounts</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_GenerateQuotation</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_QuotationNeeded</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Outlet" name="Outlet / Barista">
        <bpmn:flowNodeRef>Start_Company</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ReceiveOrder</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_CheckAvailability</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Available</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_SuggestAlternatives</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_PrepareOrder</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_PackageOrder</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>
    <bpmn:startEvent id="Start_Company" name="Order Received">
      <bpmn:messageEventDefinition />
    </bpmn:startEvent>
    <bpmn:task id="Task_ReceiveOrder" name="Receive Order" />
    <bpmn:task id="Task_CheckAvailability" name="Check Menu Availability" />
    <bpmn:exclusiveGateway id="Gateway_Available" name="Item Available?" />
    <bpmn:task id="Task_SuggestAlternatives" name="Suggest Alternatives" />
    <bpmn:task id="Task_IdentifyCustomer" name="Identify Customer" />
    <bpmn:task id="Task_CheckLoyalty" name="Check Loyalty / Membership" />
    <bpmn:task id="Task_ApplyDiscounts" name="Apply Discounts / Promotions" />
    <bpmn:task id="Task_RecordPayment" name="Record Payment" />
    <bpmn:task id="Task_PrepareOrder" name="Prepare Coffee / Food" />
    <bpmn:task id="Task_PackageOrder" name="Package Order" />
    <bpmn:task id="Task_UpdateInventory" name="Update Inventory" />
    <bpmn:task id="Task_GenerateQuotation" name="Generate Quotation" />
    <bpmn:exclusiveGateway id="Gateway_QuotationNeeded" name="Quotation Needed?" />
    <bpmn:endEvent id="End_Company" name="Order Completed">
      <bpmn:incoming>SF_15b</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:task id="Task_TriggerRestock" name="Trigger Restock Order">
      <bpmn:incoming>SF_15a</bpmn:incoming>
    </bpmn:task>
    <bpmn:exclusiveGateway id="Gateway_StockLow" name="Stock Low?">
      <bpmn:outgoing>SF_15a</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:task id="Task_GenerateInvoice" name="Generate Invoice / Receipt" />
    <bpmn:endEvent id="End_PaymentFailed" name="Payment Failed">
      <bpmn:terminateEventDefinition />
    </bpmn:endEvent>
    <bpmn:task id="Task_HandOverOrder" name="Hand Over / Deliver Order" />
    <bpmn:exclusiveGateway id="Gateway_PaymentSuccess" name="Payment Successful?" />
    <bpmn:sequenceFlow id="SF_01" sourceRef="Start_Company" targetRef="Task_ReceiveOrder" />
    <bpmn:sequenceFlow id="SF_02" sourceRef="Task_ReceiveOrder" targetRef="Task_CheckAvailability" />
    <bpmn:sequenceFlow id="SF_03" sourceRef="Task_CheckAvailability" targetRef="Gateway_Available" />
    <bpmn:sequenceFlow id="SF_03a" sourceRef="Gateway_Available" targetRef="Task_SuggestAlternatives">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${!available}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="SF_03b" sourceRef="Gateway_Available" targetRef="Task_IdentifyCustomer">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${available}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="SF_03c" sourceRef="Task_SuggestAlternatives" targetRef="Task_IdentifyCustomer" />
    <bpmn:sequenceFlow id="SF_04" sourceRef="Task_IdentifyCustomer" targetRef="Task_CheckLoyalty" />
    <bpmn:sequenceFlow id="SF_05" sourceRef="Task_CheckLoyalty" targetRef="Task_ApplyDiscounts" />
    <bpmn:sequenceFlow id="SF_06" sourceRef="Task_ApplyDiscounts" targetRef="Gateway_QuotationNeeded" />
    <bpmn:sequenceFlow id="SF_06b" sourceRef="Gateway_QuotationNeeded" targetRef="Task_RecordPayment">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${!quotationRequired}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="SF_07" sourceRef="Task_GenerateQuotation" targetRef="Task_RecordPayment" />
    <bpmn:sequenceFlow id="SF_08" sourceRef="Task_RecordPayment" targetRef="Gateway_PaymentSuccess" />
    <bpmn:sequenceFlow id="SF_10" sourceRef="Task_GenerateInvoice" targetRef="Task_PrepareOrder" />
    <bpmn:sequenceFlow id="SF_11" sourceRef="Task_PrepareOrder" targetRef="Task_PackageOrder" />
    <bpmn:sequenceFlow id="SF_12" sourceRef="Task_PackageOrder" targetRef="Task_HandOverOrder" />
    <bpmn:sequenceFlow id="SF_13" sourceRef="Task_HandOverOrder" targetRef="Task_UpdateInventory" />
    <bpmn:sequenceFlow id="SF_14" sourceRef="Task_UpdateInventory" targetRef="Gateway_StockLow" />
    <bpmn:sequenceFlow id="SF_06a" sourceRef="Gateway_QuotationNeeded" targetRef="Task_GenerateQuotation">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${quotationRequired}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="SF_15b" sourceRef="Gateway_StockLow" targetRef="End_Company">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${!stockLow}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="SF_15a" sourceRef="Gateway_StockLow" targetRef="Task_TriggerRestock">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${stockLow}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="SF_09" sourceRef="Gateway_PaymentSuccess" targetRef="Task_GenerateInvoice">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${paymentSuccess}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="SF_09b" sourceRef="Gateway_PaymentSuccess" targetRef="End_PaymentFailed">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${!paymentSuccess}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
  </bpmn:process>
