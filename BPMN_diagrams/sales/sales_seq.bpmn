      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_Invoice_Payment_di" bpmnElement="Flow_Invoice_Payment">
        <di:waypoint x="1310" y="315" />
        <di:waypoint x="1310" y="385" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_Payment_End_di" bpmnElement="Flow_Payment_End">
        <di:waypoint x="1310" y="435" />
        <di:waypoint x="1310" y="492" />
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
  <bpmndi:BPMNDiagram>
    <bpmndi:BPMNPlane bpmnElement="SubProcess_POS" />
  </bpmndi:BPMNDiagram>
</bpmn:definitions>
