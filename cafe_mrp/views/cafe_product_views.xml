<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <record id="view_cafe_product_form" model="ir.ui.view">
    <field name="name">cafe.product.form</field>
    <field name="model">cafe.product</field>
    <field name="arch" type="xml">
      <form string="Cafe Product">
        <sheet>
          <group>
            <field name="name"/>
            <field name="description"/>
            <field name="product_type"/>
            <field name="price"/>
            <!-- <field name="uom_id"/> -->
          </group>
        </sheet>
      </form>
    </field>
  </record>

  <record id="view_cafe_product_list" model="ir.ui.view">
    <field name="name">cafe.product.list</field>
    <field name="model">cafe.product</field>
    <field name="arch" type="xml">
      <list>
        <field name="name"/>
        <field name="product_type"/>
        <field name="price"/>
        <!-- <field name="uom_id"/> -->
      </list>
    </field>
  </record>
</odoo>
