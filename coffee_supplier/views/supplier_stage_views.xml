<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <data>
    <record id='view_coffee_supplier_stage_list' model='ir.ui.view'>
      <field name='name'>coffee.supplier.stage.list</field>
      <field name='model'>coffee.supplier.stage</field>
      <field name='arch' type='xml'>
        <list>
          <field name='name'/>
          <field name='sequence'/>
          <field name='fold'/>
        </list>
      </field>
    </record>

    <record id='view_coffee_supplier_stage_form' model='ir.ui.view'>
      <field name='name'>coffee.supplier.stage.form</field>
      <field name='model'>coffee.supplier.stage</field>
      <field name='arch' type='xml'>
        <form>
          <sheet>
            <group>
              <field name='name'/>
              <field name='sequence'/>
              <field name='fold'/>
            </group>
          </sheet>
        </form>
      </field>
    </record>
  </data>
</odoo>
